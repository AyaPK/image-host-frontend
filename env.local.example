# Copy this file to .env.local and fill in real values for production
# For local development with Firebase Emulator Suite, you can leave API keys blank

NEXT_PUBLIC_FIREBASE_API_KEY=
NEXT_PUBLIC_FIREBASE_AUTH_DOMAIN=your-project-id.firebaseapp.com
NEXT_PUBLIC_FIREBASE_PROJECT_ID=your-project-id
NEXT_PUBLIC_FIREBASE_STORAGE_BUCKET=your-project-id.appspot.com
NEXT_PUBLIC_FIREBASE_APP_ID=

# Emulator toggles
# Set to 1 to force client SDK to connect to local emulators
NEXT_PUBLIC_USE_EMULATORS=1
NEXT_PUBLIC_EMULATOR_HOST=127.0.0.1
NEXT_PUBLIC_FIRESTORE_EMULATOR_PORT=8080
NEXT_PUBLIC_STORAGE_EMULATOR_PORT=9199
